{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 7
	},
	"extends": [
		"airbnb"
	],
	"globals": {
		"document": true,
		"window": true,
		"process": true,
		"commonjs": true
	},
	"env": {
		"browser": true,
		"node": true,
		"jasmine": true
	},
	"rules": {
		"react/jsx-indent": [
			"error",
			"tab"
		],
		"react/prop-types": 0,
		"react/jsx-indent-props": [
			"error",
			"tab"
		],
		"react/jsx-filename-extension": 0,
		"react/jsx-no-undef": 0,
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"no-tabs": 0
	}
}
